{
  "name": "@pisa-research/utils",
  "version": "0.1.8-alpha.8",
  "description": "Misc utilities used in the PISA project",
  "author": "PISA Research Ltd",
  "homepage": "https://github.com/PISAresearch/pisa/tree/master/packages/utils#readme",
  "license": "ISC",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PISAresearch/pisa.git"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "NODE_ENV=test mocha -r ts-node/register './__tests__/**/*.test.ts' --exit",
    "test-coverage": "NODE_ENV=test nyc mocha -r ts-node/register --reporter mocha-junit-reporter --reporter-options mochaFile=coverage/mocha/test-results.xml './__tests__/**/*.test.ts' --exit"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json-summary",
      "lcovonly"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "bugs": {
    "url": "https://github.com/PISAresearch/pisa/issues"
  },
  "dependencies": {
    "@pisa-research/errors": "^0.1.8-alpha.3",
    "bunyan": "^1.8.12"
  },
  "devDependencies": {
    "@pisa-research/test-utils": "^0.1.8-alpha.3",
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.1.6",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.22.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.3"
  },
  "gitHead": "2c3b330870ae5c6eb6c940c79a459f8b46c0f5a3"
}
