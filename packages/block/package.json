{
  "name": "@pisa-research/block",
  "version": "0.1.8-alpha.6",
  "description": "Block utilites used in the PISA project",
  "author": "PISA Research Ltd",
  "homepage": "https://github.com/PISAresearch/pisa/tree/master/packages/block#readme",
  "license": "ISC",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PISAresearch/pisa.git"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "NODE_ENV=test mocha -r ts-node/register './__tests__/**/*.test.ts' --exit",
    "test-coverage": "NODE_ENV=test nyc mocha -r ts-node/register --reporter mocha-junit-reporter --reporter-options mochaFile=coverage/mocha/test-results.xml './__tests__/**/*.test.ts' --exit"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json-summary",
      "lcovonly"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "bugs": {
    "url": "https://github.com/PISAresearch/pisa/issues"
  },
  "dependencies": {
    "@pisa-research/errors": "^0.1.8-alpha.3",
    "@pisa-research/utils": "^0.1.8-alpha.6",
    "encoding-down": "^6.0.2",
    "ethers": "^4.0.37",
    "levelup": "^4.0.1",
    "subleveldown": "^4.1.0"
  },
  "devDependencies": {
    "@pisa-research/test-utils": "^0.1.8-alpha.3",
    "@types/chai": "^4.1.6",
    "@types/encoding-down": "^5.0.0",
    "@types/levelup": "^3.1.0",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.22.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.3"
  },
  "gitHead": "75cc5139b9fb14e1daeb010e201def8b5112032a"
}
